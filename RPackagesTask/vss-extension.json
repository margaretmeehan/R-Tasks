{
    "manifestVersion": 1,
    "id": "install-r-packages",
    "name": "Install R Packages",
    "version": "1.0.0",
    "publisher": "cse-nyc",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Install specified R packages from CRAN",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "Rlogo-small.png"        
    },
    "files": [
        {
            "path": "InstallRPackages"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/margaretmeehan/R-Tasks/tree/master/RPackagesTask"
    },
    "contributions": [
        {
            "id": "install-r-packages-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "InstallRPackages"
            }
        }
    ]
}